{
  "name": "ai-aggregator",
  "version": "1.0.0",
  "description": "Aggregator for agentic AI news, tools, apps, and courses",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "start:server": "node server/server.js",
    "start:client": "cd client && npm start",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "scrape": "node server/scripts/scrape.js"
  },
  "keywords": ["ai", "aggregator", "news", "tools", "scraper"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.1"
  }
}
